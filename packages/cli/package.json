{
  "name": "@ifake/pkg",
  "version": "0.0.1",
  "private": false,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "description": "A scaffolding for building front-end libraries, with javascript and typescript support.",
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "start": "tsc -w -p ./tsconfig.json"
  },
  "bin": {
    "pkg": "lib/index.js",
    "pkg-cli": "lib/index.js"
  },
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ifakejs/pkg.git"
  },
  "keywords": [
    "scaffold",
    "javascript-library",
    "typescript-library",
    "javascript",
    "typescript",
    "webpack",
    "cli",
    "npm"
  ],
  "author": "BiYuqi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ifakejs/pkg/issues"
  },
  "homepage": "https://github.com/ifakejs/pkg#readme",
  "dependencies": {
    "@ifake/pkg-eslint": "^0.0.1",
    "@ifake/pkg-package": "^0.0.1",
    "@ifake/pkg-shared": "^0.0.1",
    "@ifake/pkg-template": "^0.0.1",
    "@ifake/pkg-test": "^0.0.1",
    "@ifake/pkg-ui": "^0.0.1",
    "commander": "^6.2.0",
    "inquirer": "^7.3.3",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "^7.3.1",
