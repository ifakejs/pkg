{
  "name": "{{pkgName}}",
  "version": "0.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    {{#if eslint}}
    "lint": "eslint src --fix --ext .ts,.tsx",
    "prettier": "prettier --write --config .prettierrc.js \"src/**/*.{js,ts}\"",
    {{/if}}
    {{#if jest}}
    "test": "jest",
    {{/if}}
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./build/dev.js",
    "build": "cross-env NODE_ENV=production webpack --config ./build/prod.js"
  },
  {{#if scopePkg}}
  "publishConfig": {
    "access": "public"
  },
  {{/if}}
  "files": [
    "dist"
  ],
  "keywords": [],
  "publishConfig": {
    "access": "public"
  },
  "author": "",
  {{#if eslint}}
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.ts": [
      "prettier --write --config .prettierrc.js \"src/**/*.{js,ts}\"",
      "yarn run lint"
    ]
  }
  {{/if}}
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    {{#if jest}}
    "babel-jest": "^26.6.3",
    {{/if}}
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    {{#if eslint}}
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    {{/if}}
    "html-webpack-plugin": "^4.5.0",
    {{#if eslint}}
    "husky": "^4.3.0",
    {{/if}}
    {{#if jest}}
    "jest": "^26.6.3",
    {{/if}}
    {{#if eslint}}
    "lint-staged": "^10.5.1",
    {{/if}}
    "mini-css-extract-plugin": "^1.3.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    {{#if eslint}}
    "prettier": "^2.1.2",
    {{/if}}
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^2.3.8",
    "url-loader": "^4.1.1",
    "webpack": "^4.44.2",
    "webpack-chain": "^6.5.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
